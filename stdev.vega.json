{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "data": {
    "url": "https://raw.githubusercontent.com/BorrowSanitizer/feb-update/refs/heads/main/data.csv"
  },
  "transform": [
    { "filter": { "field": "mode", "oneOf": ["BorrowSanitizer", "Miri"] } }
  ],
  "encoding": { "y": { "field": "mode", "type": "ordinal", "title": null } },
  "layer": [
    {
      "mark": { "type": "point", "filled": true },
      "encoding": {
        "x": {
          "aggregate": "mean",
          "field": "time",
          "type": "quantitative",
          "scale": { "zero": false }
        },
        "color": { "value": "black" }
      }
    },
    {
      "mark": { "type": "errorbar", "extent": "stdev" },
      "encoding": {
        "x": {
          "field": "time",
          "type": "quantitative",
          "title": "Mean and St.Dev for Execution Time - sec"
        }
      }
    }
  ]
}
