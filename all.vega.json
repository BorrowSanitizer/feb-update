{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "A vertical box plot showing median, min, and max body mass of penguins.",
  "data": {
    "url": "https://raw.githubusercontent.com/BorrowSanitizer/feb-update/refs/heads/main/data.csv"
  },
  "mark": {
    "type": "bar"
  },
  "encoding": {
    "y": {
      "field": "mode",
      "type": "nominal",
      "sort": [
        "Release",
        "Debug",
        "AddressSanitizer",
        "ThreadSanitizer",
        "BorrowSanitizer",
        "Miri"
      ],
      "title": null
    },
    "color": {
      "field": "mode",
      "type": "nominal",
      "legend": null
    },
    "x": {
      "field": "time",
      "type": "quantitative",
      "aggregate": "median",
      "scale": {
        "type": "log"
      },
      "title": "Execution Time - log(sec)"
    }
  }
}
